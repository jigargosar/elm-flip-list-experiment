{
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "singleQuote": true,
  "semi": false,
  "printWidth": 75,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "endOfLine": "lf",
  "quoteProps": "as-needed",
  "htmlWhitespaceSensitivity": "ignore",
  "jsxSingleQuote": false,
  "proseWrap": "always"
}
